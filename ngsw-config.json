{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "newsApi",
      "urls": [
        "https://newsapi.org/v2/**"
      ],
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "15m",
        "timeout": "10s",
        "strategy": "performance"
      }
    }
  ]
}
